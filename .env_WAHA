# Engine WhatsApp
WHATSAPP_DEFAULT_ENGINE=GOWS
# Eventos enviados ao webhook
WHATSAPP_HOOK_EVENTS=message
# Proteção de API do WAHA (use um segredo forte)
WAHA_API_KEY=waha-secret
# Dashboard e swagger sem senha (altere se quiser proteger)
WAHA_DASHBOARD_NO_PASSWORD=true
WHATSAPP_SWAGGER_NO_PASSWORD=true
# Auto start das sessões
WHATSAPP_RESTART_ALL_SESSIONS=True
WHATSAPP_START_SESSION=default
# Delay entre inícios automáticos (segundos)
WAHA_AUTO_START_DELAY_SECONDS=3
